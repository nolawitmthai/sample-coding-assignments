#include <iostream>
#include <vector>
using namespace std;

void removeEvens(const vector<int> &nums, vector<int> &odds);

int main() {

	vector<int> input(9, 0);
	vector<int> result;
	int NUM_VALS = 0;
	
	cin >> NUM_VALS;
	
	for (size_t i = 0; i < NUM_VALS; ++i) {
		cin>> input.at(i);
	}

	removeEvens(input, result);

	return 0;
}

void removeEvens(const vector<int> &nums, vector<int> &odds) {
   unsigned int i = 0;
   unsigned int j = 0;
   int tempVal = 0;
      
   for (i = 0; i < nums.size(); i++) {
      if (((abs(nums.at(i) % 2))) != 1) {
      }
      else {
         tempVal = nums.at(i);
         odds.push_back(tempVal);
         cout << odds.at(j);
         
         j++;
      }
   }
}
